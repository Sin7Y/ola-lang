contract MultInputExample {

    struct Transaction {
        address sender;
        u32 nonce;
        u32 version;
        u32 chainid;
        fields data; 
        fields codes; 
        fields signature;
        hash codeHash; 
    }
    fn foo(Transaction t)  {  
        print(t.sender);
        print(t.nonce);
        print(t.version);
        print(t.chainid);
        print(t.data);
        print(t.codes);
        print(t.signature);
        print(t.codeHash);

    }

}